[Unit]
Description=Mirantis ceph historic ops dumper
After=multi-user.target

[Service]
Environment=PYTHONUNBUFFERED=1
Type=simple
User={USER}
ExecStart={PYTHON} /tmp/ceph_ho_dumper.py --log-level DEBUG --log {LOG_FILE} record --duration {DURATION} --size {SIZE} {PRIMARY} {RESULT}
StandardOutput=null
